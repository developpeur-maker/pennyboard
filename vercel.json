{
  "functions": {
    "api/pennylane.ts": {
      "maxDuration": 30,
      "runtime": "nodejs18.x"
    },
    "api/test.ts": {
      "maxDuration": 10,
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
